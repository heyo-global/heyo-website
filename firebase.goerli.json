{
  "hosting": {
    "site": "goerli-ebunker",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/dashboard",
        "destination": "/dashboard.html"
      },
      {
        "source": "/stake",
        "destination": "/stake.html"
      },
      {
        "source": "/stake/upload",
        "destination": "/stake/upload.html"
      },
      {
        "source": "/stake/create",
        "destination": "/stake/create.html"
      },
      {
        "source": "/observer",
        "destination": "/observer.html"
      },
      {
        "source": "/faq",
        "destination": "/faq.html"
      },
      {
        "source": "/privacy",
        "destination": "/privacy.html"
      },
      {
        "source": "/service",
        "destination": "/service.html"
      }
    ]
  }
}